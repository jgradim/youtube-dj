<div id="record-controls">
  <button id="record-set">Record Set</button>
  <button id="play-set">Play Set</button>
</div>

<div class="player" id="player-left">

  <div class="volume"></div>
  
  <div class="video">
    <div id="video-div-1"></div>
  </div>
  
  <div class="controls" data-player-id="1">
    <div class="loop"></div>
    <button class="play" data-playing="false">Play/Pause</button>
    <button class="mute" data-muted="false">Mute/Unmute</button>
    <button class="next">Next Song</button>
    <input type="checkbox" class="loop">
    <label>Loop</label>
  </div>
  
  <ol class="queue">
  </ol>
  
</div>

